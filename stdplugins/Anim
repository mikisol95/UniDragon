@borg.on(events.NewMessage(pattern=r"\.(.*)", outgoing=True))
async def _(event):
    if event.fwd_from:
        return
    animation_interval = 2
    animation_ttl = range(0, 100)
    input_str = event.pattern_match.group(1)
    if input_str == "eye":
        await event.edit(input_str)
        animation_chars = [

